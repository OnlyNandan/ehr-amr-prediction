# Environment variables for development
FLASK_ENV=development
FLASK_DEBUG=1
SECRET_KEY=dev-secret-key-change-in-production

# Model paths
YOLO_MODEL_PATH=models/trained/yolo_blood_smear.pt
AMR_MODEL_PATH=models/trained/amr_model.joblib
AMR_CALIBRATOR_PATH=models/trained/amr_calibrator.joblib

# Inference settings
YOLO_CONFIDENCE_THRESHOLD=0.5
